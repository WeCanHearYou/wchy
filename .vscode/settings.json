// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "data": true,
    "gin-bin": true,
    "gin-bin.exe": true,
    "gin-bin.exe~": true,
    "fider": true,
    "fider.exe": true,
    "tmp": true,
    "node_modules": true,
    "dist": true,
    "output": true,
    "coverage.txt": true,
    "debug": true,
    "**/debug.test": true,
    ".cover": true,
    "cover.out": true,
    "vendor": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "vendor": true
  },
  "vsicons.presets.angular": true,
  "go.coverOnSave": true,
  "go.coverageOptions": "showBothCoveredAndUncoveredCode",
  "go.testEnvFile": "${workspaceRoot}/.test.env",
  "go.lintTool": "golint"
}